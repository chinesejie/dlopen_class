{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/dlopen_clazz",
            "args": [],
            "stopAtEntry": false,
            "logging": { "trace": true, "traceResponse": true ,"engineLogging": true},
            "cwd": "${workspaceRoot}",
            "externalConsole": true,
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "linux": { "MIMode": "gdb" },
            "osx": { "MIMode": "gdb" },
            "windows": { "MIMode": "gdb" },
            "pipeTransport": {
                "pipeCwd": "/Users/xxx/",
                "pipeProgram": "/Users/xxx/relay.expect",
                "pipeArgs": [],
                "debuggerPath": "/home/work/.jumbo/bin/gdb"
            }
        }
    ]
}